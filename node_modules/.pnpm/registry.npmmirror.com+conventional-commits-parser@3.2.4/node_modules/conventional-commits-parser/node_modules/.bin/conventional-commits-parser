#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/mac/Desktop/myProject/npm/tyf-nb-batch-script/node_modules/.pnpm/registry.npmmirror.com+conventional-commits-parser@3.2.4/node_modules/conventional-commits-parser/node_modules:/Users/mac/Desktop/myProject/npm/tyf-nb-batch-script/node_modules/.pnpm/registry.npmmirror.com+conventional-commits-parser@3.2.4/node_modules:/Users/mac/Desktop/myProject/npm/tyf-nb-batch-script/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/mac/Desktop/myProject/npm/tyf-nb-batch-script/node_modules/.pnpm/registry.npmmirror.com+conventional-commits-parser@3.2.4/node_modules/conventional-commits-parser/node_modules:/Users/mac/Desktop/myProject/npm/tyf-nb-batch-script/node_modules/.pnpm/registry.npmmirror.com+conventional-commits-parser@3.2.4/node_modules:/Users/mac/Desktop/myProject/npm/tyf-nb-batch-script/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../cli.js" "$@"
else
  exec node  "$basedir/../../cli.js" "$@"
fi
